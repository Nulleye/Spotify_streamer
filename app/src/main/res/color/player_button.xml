<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:state_enabled="false" android:color="#FF999999"/> <!-- disabled -->
    <item android:state_pressed="true" android:color="#FFE07025"/> <!-- pressed -->
    <item android:state_checked="true" android:color="#FFE07025"/> <!-- checked -->
    <item android:state_focused="true" android:color="#FFE07025"/> <!-- focused -->
    <item android:color="#ff000000"/> <!-- default -->
</selector>

